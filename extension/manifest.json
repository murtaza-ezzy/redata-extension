{
    "name": "ReData",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["activeTab","tabs"],
    "action": {
      "default_popup": "./build/index.html",
      "default_width": 600,
      "default_height": 800
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
}